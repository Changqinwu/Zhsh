<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.qiming.wcq.mymapdemo.Fragment.MemberCenterFragment">

    <include
        layout="@layout/bottom_tab_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true">

        <ImageView
            android:id="@+id/ima_exit_app"
            android:layout_width="@dimen/home_fragment_exit_app_ima_width"
            android:layout_height="@dimen/home_fragment_exit_app_ima_height"
            android:background="#00000000"
            />

    </FrameLayout>

</RelativeLayout>
